<html>
  <head>
    <title>Homepage</title>
    <link rel="stylesheet" href="./styles.css" />
  </head>
  <body>
    <h1>Homepage</h1>
    <a href="/new">
      <span>Write New</span>
    </a>
    <ul>
      <%if (locals.jokes) {%> <% jokes.forEach((joke) => { %>
      <li>
        <p><%= joke.setup %></p>
        <p>Shared by <%= joke.poster %></p>
        <p>Posted <%= joke.timestamp %></p>
        <a href="/<%= joke.id %>/detail"">Detail</a>
      </li>
      <% });%> <% }%>
    </ul>
    <%- include('footer') %>
  </body>
</html>
