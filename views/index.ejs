<html>
  <head>
    <title>Homepage</title>
    <link rel="stylesheet" href="./styles.css" />
  </head>
  <body>
    <h1>Homepage</h1>
    <a href="/new">
      <span>Write New</span>
    </a>
    <ul>
      <% messages.forEach((message) => { %>
      <li>
        <p><%= message.text %></p>
        <p><%= message.user %></p>
        <p><%= message.added %></p>
        <a href="/detail ">Details</a>
      </li>
      <% });%>
    </ul>
    <%- include('footer') %>
  </body>
</html>
